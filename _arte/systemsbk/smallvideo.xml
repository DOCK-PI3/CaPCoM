<?xml version="1.0" encoding="UTF-8"?>
<theme>
  <formatVersion>7</formatVersion>
  <!-- <include>./animatedbackgroundsubset.xml</include> -->
  <view name="system">
    <video name="videobox" extra="true">
      <default>./_arte/sistemasvideos/default.mp4</default> 
	  <path>{random}</path>
      <delay>0.20</delay>
      <color>FFFFFFFF</color>
      <effect>none</effect>
	  <size>0.35 0.44</size>
	  <maxSize>0.42 0.440</maxSize>
      <origin>0 0</origin>
	  <pos>0.325 0.29</pos>
      <roundCorners>0.03</roundCorners>
      <visible>true</visible>
    </video>
    <image name="ControllerOverlay" extra="true">
      <visible>false</visible>
    </image>
  </view>
</theme>
